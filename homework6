my_dict = {"Anna": 1976, "Larisa": 1935, "Diana": 2004, "Katya": 1983}
print("Dict:", my_dict)
print('Existing value:', my_dict["Anna"])
print("Not existing value:", my_dict.get("Lena"))
my_dict.update({"Glasha": 2013, "Liza": 1999})
a = my_dict.pop("Liza")
print('Deleted value:', a)
print('Modified dictionary:', my_dict)

my_set = {1, 1, True, "cat", True, "cat", 1, 2}
print('Set:', my_set)
my_set.update({4, 2.7, "dog", 2.7, "dog"})
my_set.remove(2)
print('Modified set:', my_set)
